{
  "name": "@blastz/nico-build",
  "version": "0.2.0",
  "description": "nico build utility",
  "main": "src/index.js",
  "scripts": {
    "tsc": "tsc",
    "push": "npm run build && cd ./dist && npm publish --access public --registry=https://registry.npmjs.org",
    "test": "jest",
    "build": "rm -rf dist && npm run tsc && cp package.json README.md ./dist && cp -r ./template ./dist/src"
  },
  "keywords": [
    "node",
    "build",
    "nico"
  ],
  "author": "blastz",
  "license": "ISC",
  "dependencies": {
    "@zeit/ncc": "^0.22.3",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@blastz/nico": "^0.9.4",
    "@types/jest": "^26.0.0",
    "@types/shelljs": "^0.8.8",
    "javascript-obfuscator": "^1.1.0",
    "jest": "^26.0.1",
    "supertest": "^4.0.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "src/**/*"
  ]
}
